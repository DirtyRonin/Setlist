  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./dist/",
    "allowSyntheticDefaultImports" : true,
    "esModuleInterop" : true,
    "preserveConstEnums": true,
    "strict": true,
